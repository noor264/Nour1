
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Playpiano">


    <Button
        android:id="@+id/c"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_marginStart="150dp"
        android:layout_marginEnd="54dp"
        android:backgroundTint="@color/white"
        android:onClick="playc"
        android:shadowColor="@color/black"
        android:text="   C"
        android:textAlignment="textStart"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/d"
        app:layout_constraintEnd_toStartOf="@+id/ds"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/d"
        android:layout_width="90dp"
        android:layout_height="0dp"
        android:layout_marginStart="90dp"
        android:layout_marginEnd="90dp"
        android:backgroundTint="@color/white"
        android:onClick="playd"
        android:shadowColor="@color/black"
        android:text="   D"
        android:textAlignment="textStart"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/e"
        app:layout_constraintStart_toStartOf="@+id/c"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/e"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_marginEnd="365dp"
        android:backgroundTint="@color/white"
        android:onClick="playe"
        android:text="   E"
        android:textAlignment="textStart"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/d"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/fs"
        app:layout_constraintStart_toEndOf="@+id/cs" />

    <Button
        android:id="@+id/f"
        android:layout_width="90dp"
        android:layout_height="match_parent"
        android:layout_marginStart="90dp"
        android:layout_marginEnd="90dp"
        android:backgroundTint="@color/white"
        android:onClick="playf"
        android:text="   F"
        android:textAlignment="textStart"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/e"
        app:layout_constraintEnd_toEndOf="@+id/g"
        app:layout_constraintStart_toStartOf="@+id/e" />

    <Button
        android:id="@+id/g"
        android:layout_width="90dp"
        android:layout_height="match_parent"
        android:layout_marginStart="90dp"
        android:layout_marginEnd="90dp"
        android:backgroundTint="@color/white"
        android:onClick="playg"
        android:text="   G"
        android:textAlignment="textStart"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/f"
        app:layout_constraintEnd_toEndOf="@+id/a"
        app:layout_constraintStart_toEndOf="@+id/e" />

    <Button
        android:id="@+id/a"
        android:layout_width="90dp"
        android:layout_height="match_parent"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="31dp"
        android:backgroundTint="@color/white"
        android:onClick="playa"
        android:text="   A"
        android:textAlignment="textStart"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/g"
        app:layout_constraintEnd_toEndOf="@+id/as"
        app:layout_constraintStart_toStartOf="@+id/gs" />

    <Button
        android:id="@+id/b"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_marginEnd="5dp"
        android:backgroundTint="@color/white"
        android:onClick="playb"
        android:shadowColor="@color/black"
        android:text="   B"
        android:textAlignment="textStart"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/a"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/as" />

    <!--        app:layout_constraintBottom_toBottomOf="@+id/cs"-->
    <!--        app:layout_constraintEnd_toEndOf="parent"-->
    <!--        app:layout_constraintStart_toEndOf="@+id/ds"-->
    <!--        app:layout_constraintTop_toTopOf="@+id/cs"-->
    <!--        app:layout_constraintVertical_bias="1.0" />-->

    <Button
        android:id="@+id/cs"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="205dp"
        android:layout_marginEnd="55dp"
        android:backgroundTint="@color/black"
        android:onClick="playcs"
        android:text="   C#"
        android:textAlignment="textStart"
        android:textColor="#ffffff"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/ds"
        app:layout_constraintEnd_toStartOf="@+id/e"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ds" />

    <Button
        android:id="@+id/ds"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:backgroundTint="@color/black"
        android:onClick="playds"
        android:text="   D#"
        android:textAlignment="textStart"
        android:textColor="#ffffff"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/fs"
        app:layout_constraintEnd_toStartOf="@+id/e"
        app:layout_constraintStart_toEndOf="@+id/c"
        app:layout_constraintTop_toTopOf="@+id/fs" />

    <Button
        android:id="@+id/fs"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="29dp"
        android:backgroundTint="@color/black"
        android:onClick="playfs"
        android:text="   F#"
        android:textAlignment="textStart"
        android:textColor="#ffffff"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/gs"
        app:layout_constraintEnd_toStartOf="@+id/gs"
        app:layout_constraintStart_toEndOf="@+id/e"
        app:layout_constraintTop_toTopOf="@+id/gs" />

    <Button
        android:id="@+id/gs"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="21dp"
        android:backgroundTint="@color/black"
        android:onClick="playgs"
        android:text="   G#"
        android:textAlignment="textStart"
        android:textColor="#ffffff"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/as"
        app:layout_constraintEnd_toStartOf="@+id/as"
        app:layout_constraintStart_toEndOf="@+id/fs"
        app:layout_constraintTop_toTopOf="@+id/as" />

    <Button
        android:id="@+id/as"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="@color/black"
        android:onClick="playas"
        android:text="   A#"
        android:textAlignment="textStart"
        android:textColor="#ffffff"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/b"
        app:layout_constraintStart_toEndOf="@+id/gs"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>